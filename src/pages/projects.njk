---
layout : 'base.njk'
title : "PlumbLab | Projects"

heroImgSrc: "/assets/about.jpg"
heroImgAlt: "HomePageHeroImg"
eleventyComputed:
 heroTitle: "{{ title }}"

---

<!-- hero section -->
{% include "_layouts/heroSection.njk" %}



<!-- projects gallary -->
<section class=" mx-auto  py-16 bg-[var(--bgLight)]  px-4 md:px-14">
    <div class="mb-10">
        <h2 class="text-center text-4xl mb-3 text-[var(--colorDark)] font-bold">Our Latest <span
